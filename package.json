{
  "name": "star-wars-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://Nazarsenychak11.github.io/star-wars-app",
  "dependencies": {
    "eslint-plugin-react-hooks": "^1.0.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
